                    {
                      "name": "CRONJOBS"
                    },
                    {
                      "name": "SERVICE_NAME",
                      "value": "node"
                    }
                  ],
                  "resources": {
                    "requests": {
                      "cpu": "10m",
                      "memory": "10Mi"
                    }
                  },
                  "volumeMounts": [
                    {
                      "name": "custom-config",
                      "mountPath": "/config"
                    },
                    {
                      "name": "custom-files",
                      "mountPath": "/app/files/"
                    },
                    {
                      "name": "node",
                      "mountPath": "/data"
                    }
                  ],
                  "livenessProbe": {
                    "tcpSocket": {
                      "port": 3000
                    },
                    "initialDelaySeconds": 60,
                    "timeoutSeconds": 10
                  },
                  "readinessProbe": {
                    "tcpSocket": {
                      "port": 3000
                    },
                    "initialDelaySeconds": 1,
                    "timeoutSeconds": 1
                  },
                  "imagePullPolicy": "Always",
                  "securityContext": {}
                }
              ],
              "imagePullSecrets": [
                {
                  "name": "lagoon-internal-registry-secret"
                }
              ],
              "priorityClassName": "lagoon-priority-production",
              "enableServiceLinks": false
            }
          },
          "strategy": {}
        },
        "status": {}
      }
